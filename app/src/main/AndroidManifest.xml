        android:theme="@style/Theme.CLUBIN">
        <activity
            android:name=".login.view.LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />
        <activity
            android:name=".onboarding.view.OnboardingActivity"
            android:exported="false"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />
        <activity
            android:name=".inscription.InscriptionPasswordActivity"
            android:exported="false"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />
        <activity
